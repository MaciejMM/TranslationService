spring:
  datasource:
    url: JDBC_URL
    username: DB_USERNAME
    password: BD_PASSWORD
  jpa:
    hibernate:
      ddl-auto: create-drop
    show-sql: false
    properties:
      hibernate:
        dialect: "org.hibernate.dialect.PostgreSQLDialect"
        format_sql: true

springdoc:
  swagger-ui:
    path: "/swagger-ui.html"